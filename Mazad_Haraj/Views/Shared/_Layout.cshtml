        //    loop: true
        //});
        anime({
            targets: '.card-1',
            opacity: [0, 1],
            translateX: [24, 0],
            onview: -250,
            delay: 200
        });
        anime({
            targets: '.card-3',
            opacity: [0, 1],
            translateX: [-24, 0],
            onview: -250,
            delay: 300
        });
        anime({
            targets: '.card-2',
            opacity: [0, 1],
            translateY: [24, 0],
            onview: true,
            delay: 100
        });
        anime({
            targets: '.title',
            opacity: [0, 1],
            translateY: [24, 0],
            onview: true,
            delay: 100
        });
        const animation = anime({
            targets: '.contact-sec',
            opacity: [0, 1],
            translateY: [24, 0],
            onview: true,
            delay: 100
        });
        anime({
            targets: '.bid-box',
            opacity: [0, 1],
            scale: [0, 1],
            onview: true,
            delay: 440
        });
        anime({
            targets: '.features .btn',
            opacity: [0, 1],
            translateY: [-24, 0],
            onview: true,
            delay: 200
        });
        anime({
            targets: '.social-icons-list li',
            opacity: [0, 1],
            scale: [0, 1],
            onview: true,
            delay: 440
        });

        const section = document.querySelector(".home");

        // Animate on scroll
        const animateOnScroll = function (div, speed = 100, offset = 0) {
            const scrollPercent = window.pageYOffset - div.offsetTop;
            return (scrollPercent + offset) / speed;
        };

        // Scroll listener
        window.onscroll = function () {
            animation.seek(animateOnScroll(section, 1000, 200) * animation.duration);
        };
    </script>
